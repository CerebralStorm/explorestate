<p id="notice"><%= notice %></p>

<h1>Zip Codes</h1>

<table>
  <thead>
    <tr>
      <th>Value</th>
      <th>Median listing price</th>
      <th>Average listing price</th>
      <th>Price to rent ratio</th>
      <th>Median rent</th>
      <th>Estimated rent</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @zip_codes.each do |zip_code| %>
      <tr>
        <td><%= zip_code.value %></td>
        <td><%= zip_code.median_listing_price %></td>
        <td><%= zip_code.average_listing_price %></td>
        <td><%= zip_code.price_to_rent_ratio %></td>
        <td><%= zip_code.median_rent %></td>
        <td><%= zip_code.estimated_rent %></td>
        <td><%= link_to 'Show', zip_code %></td>
        <td><%= link_to 'Edit', edit_zip_code_path(zip_code) %></td>
        <td><%= link_to 'Destroy', zip_code, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Zip Code', new_zip_code_path %>
