<%= form_with(model: lead, local: true) do |form| %>
  <% if lead.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(lead.errors.count, "error") %> prohibited this lead from being saved:</h2>

      <ul>
      <% lead.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :parcel_number %>
    <%= form.text_field :parcel_number, id: :lead_parcel_number %>
  </div>

  <div class="field">
    <%= form.label :address %>
    <%= form.text_field :address, id: :lead_address %>
  </div>

  <div class="field">
    <%= form.label :city %>
    <%= form.text_field :city, id: :lead_city %>
  </div>

  <div class="field">
    <%= form.label :state %>
    <%= form.text_field :state, id: :lead_state %>
  </div>

  <div class="field">
    <%= form.label :zip_code_id %>
    <%= form.text_field :zip_code_id, id: :lead_zip %>
  </div>

  <div class="field">
    <%= form.label :status %>
    <%= form.text_field :status, id: :lead_status %>
  </div>

  <div class="field">
    <%= form.label :lead_type %>
    <%= form.text_field :lead_type, id: :lead_lead_type %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
